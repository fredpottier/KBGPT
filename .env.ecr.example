# Configuration pour utilisation des images ECR
# Copiez vers .env.ecr et configurez vos valeurs

# AWS Configuration
AWS_ACCOUNT_ID=123456789012  # Votre Account ID AWS
AWS_REGION=us-east-1

# Services SAP KB (conservez vos valeurs existantes)
APP_PORT=8000
FRONTEND_PORT=3000
APP_UI_PORT=8501

# Debug
DEBUG_APP=false
DEBUG_WORKER=false

# API Keys (conservez vos valeurs existantes)
OPENAI_API_KEY=your-openai-key
ANTHROPIC_API_KEY=your-anthropic-key

# Qdrant & Redis (conservez vos valeurs existantes)
QDRANT_API_KEY=
REDIS_URL=redis://redis:6379